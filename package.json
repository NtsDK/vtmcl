{
  "name": "vtmcl",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "tsc": "tsc --noEmit --watch",
    "build-tailwind": "npx tailwindcss ./tailwind.css -o ./src/tailwind.css -w",
    "start": "vite",
    "check-build": "http-server ./dist/static",
    "build": "vite build --outDir dist/static && npm run build:ssr",
    "build:ssr": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "postbuild0": "npx ts-node --esm prerender.ts",
    "postbuild": "npx tsx prerender.ts",
    "prebuild": "rm -rf dist",
    "build0": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint --quiet",
    "lint2": "eslint --fix",
    "format": "prettier --write ./src/**/*.{ts,tsx}",
    "lf": "npm run lint && npm run format",
    "prepare": "husky install",
    "test": "npx jest",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/file-saver": "^2.0.5",
    "@types/luxon": "^3.3.2",
    "@types/ramda": "^0.29.3",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "ajv": "^8.12.0",
    "autoprefixer": "^10.4.15",
    "bootstrap": "^5.3.1",
    "classnames": "^2.3.2",
    "file-saver": "^2.0.5",
    "http-server": "^14.1.1",
    "i18next": "^23.4.6",
    "i18next-browser-languagedetector": "^7.1.0",
    "luxon": "^3.4.2",
    "nanoid": "^4.0.2",
    "postcss": "^8.4.29",
    "ramda": "^0.29.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.1",
    "react-router-dom": "^6.15.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.7.3",
    "web-vitals": "^3.4.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "^16.18.48",
    "@types/react-document-title": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "prettier": "3.0.3",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "source-map-explorer": "^2.5.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "vite": "^4.4.9"
  }
}
